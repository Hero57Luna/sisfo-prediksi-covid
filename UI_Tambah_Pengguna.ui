<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="top_level">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="header_tambah_pengguna">
        <property name="anchor">w</property>
        <property name="compound">top</property>
        <property name="cursor">based_arrow_down</property>
        <property name="font">{Arial} 14 {bold}</property>
        <property name="justify">center</property>
        <property name="padding">10</property>
        <property name="text" translatable="yes">Tambah Pengguna</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">False</property>
          <property name="row">0</property>
          <property type="col" id="0" name="pad">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="input_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property type="col" id="0" name="pad">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="label_nama">
            <property name="text" translatable="yes">Nama</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">5</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
              <property type="row" id="0" name="pad">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_nama">
            <property name="exportselection">true</property>
            <property name="font">TkDefaultFont</property>
            <property name="state">normal</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property type="row" id="0" name="pad">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_username">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_username">
            <property name="text" translatable="yes">Username</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">5</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_password">
            <property name="text" translatable="yes">Password</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_password">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_konf_pass">
            <property name="text" translatable="yes">Konfirmasi Password</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">5</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_konf_pass">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_role">
            <property name="text" translatable="yes">Role</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_role">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">5</property>
              <property name="propagate">True</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Entry" id="input_departement">
            <layout manager="grid">
              <property name="column">1</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="label_departement">
            <property name="text" translatable="yes">Departement</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="button_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
          <property type="col" id="0" name="pad">0</property>
        </layout>
        <child>
          <object class="ttk.Button" id="button_submit">
            <property name="command" type="command" cbtype="simple">submit</property>
            <property name="compound">top</property>
            <property name="state">normal</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Submit</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">False</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="button_kembali">
            <property name="text" translatable="yes">Kembali</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">10</property>
              <property name="pady">10</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
